{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "baseUrl": "./",
    "paths": {
      "@/cy/*": ["*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"],
  "include": ["./**/*.ts", "types/**/*.d.ts", "../cypress.config.ts"]
}
